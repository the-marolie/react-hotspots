{
  "name": "react-hotspots",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.{js,jsx,ts,jsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,jsx}'",
    "format": "prettier --write src//**/*.{ts,jsx,css} --config ./.prettierrc"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,jsx}": [
      "npm run eslint",
      "npm run  format"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-hotspots.es.js",
      "require": "./dist/react-hotspots.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/react-hotspots.umd.js",
  "module": "./dist/react-hotspots.es.js",
  "types": "./dist/index.d.js",
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-react-scoped-css": "^0.8.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-linter": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": ".https://github.com/the-marolie/react-hotspots"
  },
  "keywords": [],
  "author": "Akshith Marolie",
  "bugs": {
    "url": ".https://github.com/the-marolie/react-hotspots/issues"
  },
  "homepage": ".https://github.com/the-marolie/react-hotspots#readme",
  "license": "MIT",
  "dependencies": {
    "buffer": "^6.0.3",
    "js-xxhash": "^2.0.0",
    "react-icons": "^4.7.1",
    "sass": "^1.58.0"
  }
}
